{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "*.{css,js,json,ico,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot}"]
  },
  "mimeTypes": {
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".json": "application/json",
    ".css": "text/css",
    ".html": "text/html"
  },
  "globalHeaders": {
    "X-StaticWebApp-Config": "loaded",
    "X-Config-Test": "FROM-FRONTEND-ROOT",
    "X-Config-Location": "frontend/staticwebapp.config.json"
  }
}
